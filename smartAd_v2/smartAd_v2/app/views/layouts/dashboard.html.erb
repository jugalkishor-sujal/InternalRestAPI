<body>
    
<!-- Preloader -->
<div id="preloader">
    <div id="status"><i class="fa fa-spinner fa-spin"></i></div>
</div>

<section>
  
  <div class="leftpanel">
    
    <div class="logopanel">
        <h1>SmartAd</span></h1>
    </div><!-- logopanel -->
        
    
	<%= render 'shared/left_panel' %>
  </div><!-- leftpanel -->
  
  <div class="mainpanel">
    
